{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CommonHeaderDefinitions",
  "definitions": {
    "version": {
      "type": "integer",
      "description": "Version",
      "bitLength": 8
    },
    "magic": {
      "type": "integer",
      "description": "Magic number",
      "bitLength": 56
    },
    "length": {
      "type": "integer",
      "description": "Length (in bytes)",
      "bitLength": 32
    },
    "headerLength": {
      "type": "integer",
      "description": "Header length (in bytes)",
      "bitLength": 16
    },
    "status": {
      "type": "integer",
      "description": "Status",
      "bitLength": 16
    },
    "type": {
      "type": "integer",
      "description": "Type",
      "bitLength": 32
    },
    "elapsedTime": {
      "type": "integer",
      "description": "Elapsed time",
      "bitLength": 32
    },
    "epochTimeSec": {
      "type": "integer",
      "description": "Linux Epoch Time in seconds",
      "bitLength": 64
    },
    "epochTimeUSec": {
      "type": "integer",
      "description": "Linux Epoch Time in microseconds",
      "bitLength": 64
    }
  }
}
